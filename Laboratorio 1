    {
      "cell_type": "code",
      "source": [
        "import matplotlib.pyplot as plt\n",
        "\n",
        "x = np.linspace (-10, 10, 100) #Generar 100 puntos de -10 a 10\n",
        "y = x**2 #funcion caudratica\n",
        "plt.plot(x,y)\n",
        "plt.title(\"Grafico de f(x)=x^2\")\n",
        "plt.xlabel(\"x\")\n",
        "plt.ylabel(\"f(x)\")\n",
        "plt.grid(True)\n",
        "plt.show()"
